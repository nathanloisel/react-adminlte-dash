{
  "env": {
    "build": {
      "presets": [
        "es2015",
        "react"
      ]
    },
    "start": {
      "presets": [
        "es2015",
        "react",
        "react-hmre"
      ],
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap"
          }
        ]
      ]
    },
    "test": {
      "presets": [
        "es2015",
        "react"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "istanbul"
      ]
    },
    "commonjs": {
      "presets": [
        "es2015",
        "react"
      ],
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": false }]
      ]
    }
  }
}
